{% extends 'base.html.twig' %}

{% block title %}Owned index{% endblock %}

{% block body %}
    <h1>Owned index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Opquan</th>
                <th>Opcreated</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for owned in owneds %}
            <tr>
                <td>{{ owned.id }}</td>
                <td>{{ owned.opquan }}</td>
                <td>{{ owned.opcreated ? owned.opcreated|date('Y-m-d H:i:s') : '' }}</td>
                <td>
                    <a href="{{ path('app_owned_show', {'id': owned.id}) }}">show</a>
                    <a href="{{ path('app_owned_edit', {'id': owned.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="4">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_owned_new') }}">Create new</a>
{% endblock %}
